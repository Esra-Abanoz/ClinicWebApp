<script type="text/javascript">
        Ext = window.Ext || {};

        $(document).ajaxSend(function (event, xhr, options) {
            var el, em = options.eventMask || {}
            if (em.showMask === true) {

                if (!Ext.isEmpty(em.customTarget, false) && Ext.isEmpty(em.target, false)) {
                    em.target = "customtarget";
                }
                switch (em.target || "page") {
                    case "page":
                        el = Ext.getBody();
                        break;
                    case "customtarget":
                        var trg = em.customTarget || "";
                        if (!Ext.isEmpty(trg)) {
                            el = Ext.getCmp(trg);
                        }
                        break;
                }
                if (!Ext.isEmpty(el)) {
                    el.mask(em.msg || Ext.view.AbstractView.prototype.loadingText, em.msgCls || Ext.LoadMask.prototype.msgCls);
                    options.maskEl = el;
                }
            }
        }).ajaxComplete(function (event, xhr, options) {
            if (options.maskEl !== undefined && options.maskEl !== null) {
                var delay = 0,
                    em = options.eventMask || {},
                    remove,
                    task;
                if (em && em.minDelay) {
                    delay = em.minDelay;
                }
                remove = (em.target || "page") == "page";
                remove = false;
                task = new Ext.util.DelayedTask(Ext.CustomFunctions.removeMaskDelay, this, [options.maskEl, remove]).delay(delay);
            }
        });

