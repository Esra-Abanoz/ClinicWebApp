@using System.IO
@using ClinicWeb.Repository.Helpers
@using ClinicWeb.Models.Enums
@using Microsoft.AspNetCore.Hosting
@using Microsoft.AspNetCore.Mvc.TagHelpers
@inherits ClinicWeb.UI.Views.RazorPageViewBase
@inject IWebHostEnvironment webHostEnvironment
@{
    var jsFileList = new List<string>();
    var jsFilesFolder = new DirectoryInfo(System.IO.Path.Combine(webHostEnvironment.WebRootPath, "EXT/PageJs"));
    if (jsFilesFolder.Exists)
    {
        var jsFiles = jsFilesFolder.GetFiles("*.js", SearchOption.AllDirectories);
        if (jsFiles != null && jsFiles.Length > 0)
        {
            foreach (var jsFile in jsFiles)
            {
                jsFileList.Add(jsFile.FullName.Replace(webHostEnvironment.WebRootPath + System.IO.Path.DirectorySeparatorChar.ToString(), "~/").Replace(System.IO.Path.DirectorySeparatorChar.ToString(), "/"));
            }
        }
    }
}
